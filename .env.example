# Qwen3-TTS Server Configuration

# Auth token (auto-generated on first run if empty)
AUTH_TOKEN=

# Bridge server URL (OpenClaw server)
BRIDGE_URL=wss://104.248.27.154:8765

# Models to load (comma-separated): voice_design, custom_voice, base, base_small
ENABLED_MODELS=voice_design,base

# Model cache directory (leave empty for HuggingFace default)
MODEL_CACHE_DIR=

# GPU device
CUDA_DEVICE=cuda:0

# Bridge HTTP port (on OpenClaw server)
BRIDGE_HTTP_PORT=8766

# Bridge WebSocket port (accepts tunnel connections)
BRIDGE_WS_PORT=8765

# Rate limit (requests per minute)
RATE_LIMIT=30

# Log level
LOG_LEVEL=INFO

# Max text length per request
MAX_TEXT_LENGTH=5000

# Voice storage directory
VOICES_DIR=./voices
